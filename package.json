{
  "name": "the-todo-app",
  "version": "1.1.0",
  "author": "JP",
  "homepage": "https://thetodoapp.com",
  "scripts": {
    "ionic:serve:before": "node scripts/node-check.js",
    "ionic:build:before": "node scripts/node-check.js",
    "ng": "ng",
    "start": "ng serve",
    "compress": "npx gzipper --verbose --gzip-level 9 --brotli ./www",
    "build": "ng build && npm run compress",
    "deploy": "ionic build --prod && npm run compress && firebase deploy --only hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
